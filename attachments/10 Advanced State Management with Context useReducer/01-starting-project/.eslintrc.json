{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  },
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings"]
}
